# config file for bash-completion
# https://github.com/scop/bash-completion

set (BASH_COMPLETION_VERSION "2.8")

set (BASH_COMPLETION_PREFIX "/usr/local")
set (BASH_COMPLETION_COMPATDIR "/etc/bash_completion.d")
set (BASH_COMPLETION_COMPLETIONSDIR "/usr/local/share/bash-completion/completions")
set (BASH_COMPLETION_HELPERSDIR "/usr/local/share/bash-completion/helpers")

set (BASH_COMPLETION_FOUND "TRUE")
